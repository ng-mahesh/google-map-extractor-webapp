# Server Configuration
PORT=3001
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/google-maps-extractor

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=86400000

# Scraper Configuration
MAX_RESULTS_PER_KEYWORD=50
SCRAPER_TIMEOUT=60000
HEADLESS_BROWSER=true

# CORS
CORS_ORIGIN=http://localhost:3000

# Optional: Redis Configuration (for job queue and caching - recommended for production)
# REDIS_HOST=localhost
# REDIS_PORT=6379

# Optional: Sentry Configuration (for error tracking - recommended for production)
# SENTRY_DSN=your-sentry-dsn-here

# Optional: Logging Configuration
# LOG_LEVEL=info
